TARGETNAME = BIBDrv
TARGETTYPE = DYNLINK
RELEASETYPE = PLATFORM
WINCEOEM = 1
DLLENTRY = _DllMainCRTStartup
DEFFILE = BIBDrv.def

!IF "$(_TGTPROJ)" == "uldr"
CDEFINES = $(CDEFINES) -DULDR
!ENDIF

CDEFINES = $(CDEFINES) -DCOREDLL
LDEFINES = -subsystem:native /DEBUG /DEBUGTYPE:CV /FIXED:NO

INCLUDES = $(INCLUDES);$(_TARGETPLATROOT)\src\inc;

TARGETLIBS = \
    $(_PUBLICROOT)\common\oak\lib\$(_CPUINDPATH)\storeapi.lib \
    $(_COMMONOAKROOT)\lib\$(_CPUINDPATH)\ceddk.lib \
    $(_COMMONSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib \
    $(_PLATLIB)\$(_CPUINDPATH)\DriverLib.lib \
    $(_PLATLIB)\$(_CPUINDPATH)\SDMMCLib_BIBDrv.lib \

SOURCES = \
    BIBDrv.c

